{
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeLensFontFamily": "'Space Mono Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Space Mono Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.lineHeight": 1.6,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.semanticHighlighting.enabled": true,
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.tabSize": 2,
  "[markdown]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "intelephense.environment.phpVersion": "8.4",
  "intelephense.phpdoc.useFullyQualifiedNames": true,
  "prettier.resolveGlobalModules": true,
  "terminal.integrated.fontFamily": "'Space Mono Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
  "workbench.productIconTheme": "Default",
  "workbench.tree.enableStickyScroll": false,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  }
}
